  "license": "(Apache-2.0 OR GPL-3.0)",
  "bugs": {
    "url": "https://github.com/epfromer/pst-mongodb/issues"
  },
  "homepage": "https://github.com/epfromer/pst-mongodb#readme",
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/long": "^4.0.0",
    "@types/mongodb": "^3.0.9",
    "@types/node": "^10.1.1",
    "@types/winston": "^2.3.9",
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "node-readme": "^0.1.9",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "source-map-support": "^0.5.5",
    "supports-color": "^5.4.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "config": "^1.30.0",
    "mongodb": "^3.1.0-beta4",
    "nodemon": "^1.17.3",
