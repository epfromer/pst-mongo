{
  "name": "pst-mongo",
  "version": "0.0.1",
  "description": "Extract objects from MS Outlook/Exchange PST files and stores in MongoDB",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "NODE_ENV=dev npm-run-all clean build --parallel watch:build watch:server --print-label",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'",
    "test": "NODE_ENV=test nyc --reporter=html mocha --opts mocha.opts",
    "readme": "node ./node_modules/.bin/node-readme"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/Log.class.ts",
      "coverage/prettify.js",
      "coverage/sorter.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfromer/pst-extractor.git"
  },
  "keywords": [],
  "author": "Ed Pfromer (epfromer@gmail.com)",
  "license": "(Apache-2.0 OR GPL-3.0)",
  "bugs": {
    "url": "https://github.com/epfromer/pst-extractor/issues"
  },
  "homepage": "https://github.com/epfromer/pst-extractor#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/config": "0.0.34",
    "@types/debug": "0.0.30",
    "@types/fs-ext": "0.0.29",
    "@types/long": "^3.0.32",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^3.0.9",
    "@types/mongoose": "^5.0.7",
    "@types/node": "^9.6.1",
    "@types/typescript": "^2.0.0",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "chai-datetime": "^1.5.0",
    "debug": "^3.1.0",
    "mocha": "^5.0.5",
    "node-readme": "^0.1.9",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "source-map-support": "^0.5.4",
    "supports-color": "^5.3.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "config": "^1.30.0",
    "fs-ext": "^1.0.0",
    "log-update": "^2.3.0",
    "long": "^4.0.0",
    "math-float64-from-bits": "^1.0.0",
    "mongoose": "^5.0.12",
    "nodemon": "^1.17.3",
    "pst-extractor": "^1.1.0",
    "rimraf": "^2.6.2",
    "uuid-parse": "^1.0.0",
    "winston": "^2.4.1",
    "winston-loggly": "^1.3.1",
    "winston-loggly-bulk": "^2.0.2"
  }
}
