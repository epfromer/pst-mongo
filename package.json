{
  "name": "pst-mongo",
  "version": "0.0.1",
  "description": "Extract objects from MS Outlook/Exchange PST files and stores in MongoDB",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "NODE_ENV=dev npm-run-all clean build --parallel watch:build watch:server --print-label",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'",
    "readme": "node ./node_modules/.bin/node-readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfromer/pst-extractor.git"
  },
  "keywords": [],
  "author": "Ed Pfromer (epfromer@gmail.com)",
  "license": "(Apache-2.0 OR GPL-3.0)",
  "bugs": {
    "url": "https://github.com/epfromer/pst-extractor/issues"
  },
  "homepage": "https://github.com/epfromer/pst-extractor#readme",
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/log-update": "^2.0.0",
    "@types/long": "^3.0.32",
    "@types/mongodb": "^3.0.9",
    "@types/node": "^9.6.2",
    "@types/typescript": "^2.0.0",
    "@types/winston": "^2.3.9",
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "node-readme": "^0.1.9",
    "npm-run-all": "^4.1.2",
    "source-map-support": "^0.5.4",
    "supports-color": "^5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "config": "^1.30.0",
    "log-update": "^2.3.0",
    "mongodb": "^3.0.5",
    "nodemon": "^1.17.3",
    "pst-extractor": "^1.2.2",
    "rimraf": "^2.6.2",
    "winston": "^2.4.1"
  }
}
