{
  "name": "pst-mongo",
  "version": "0.0.1",
  "description": "Extract objects from MS Outlook/Exchange PST files and stores in MongoDB",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "NODE_ENV=dev npm-run-all clean build --parallel watch:build watch:server --print-label",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'",
    "readme": "node ./node_modules/.bin/node-readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfromer/pst-mongodb.git"
  },
